{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "@core/*" : ["src/app/core/*"],
      "@models/*" : ["src/app/core/models/*"],
      "@modules/*" : ["src/app/modules/*"]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
